<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote name="abc"
          fetch="https://github.com/ABC-ROM"
          revision="o" />

  <!-- General repos -->
  <project path="bionic" name="bionic" remote="abc" />
